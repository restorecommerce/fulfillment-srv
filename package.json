{
  "name": "fulfillments-srv",
  "version": "0.0.1",
  "main": "./lib/start.js",
  "description": "fulfillment service for creating and managing fulfillment",
  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/fulfillment-srv"
  },
  "dependencies": {
    "@restorecommerce/cart": "^0.3.5",
    "@restorecommerce/chassis-srv": "^0.3.10",
    "@restorecommerce/cluster-service": "^0.1.13",
    "@restorecommerce/grpc-client": "^0.4.2",
    "@restorecommerce/kafka-client": "^0.4.1",
    "@restorecommerce/logger": "^0.13.0",
    "@restorecommerce/protos": "^0.7.2",
    "@restorecommerce/resource-base-interface": "^0.2.12",
    "@restorecommerce/service-config": "^0.4.27",
    "@types/node-fetch": "^2.6.2",
    "@types/soap": "^0.21.0",
    "@types/xml-js": "^1.0.0",
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.13",
    "protobufjs": "^6.11.3",
    "co": "^4.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "html2json": "^1.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "node-fetch": "^3.2.6",
    "redis": "^4.1.0",
    "request": "^2.88.2",
    "soap": "^0.45.0",
    "ts-proto-descriptors": "^1.6.0",
    "tslint": "^6.1.3",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@types/redis": "^4.0.10",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.17.0",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "long": "5.2.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "should": "^13.2.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "ts-proto": "^1.115.4"
  },
  "scripts": {
    "start": "npm run createtopics && node lib/start.js",
    "dev": "cross-env NODE_ENV=development npm run createtopics && nodemon --watch './src/**/*.ts' --exec 'ts-node' src/worker.ts",
    "dev:nocluster": "ts-node src/worker.ts",
    "lint": "eslint src --ext .ts --fix",
    "createtopics": "node setupTopics io.restorecommerce.fulfillment.resource io.restorecommerce.command io.restorecommerce.jobs io.restorecommerce.fulfillment_method.resource io.restorecommerce.fulfillment_courier.resource",
    "build:tsc": "tsc -d",
    "build:clean": "rimraf lib",
    "build": "npm-run-all build:clean build:tsc",
    "pretest": "npm run build && npm run tsctests && npm run createtopics",
    "test": " nyc npm run mocha",
    "tsctests": "tsc -d -p tsconfig.test.json",
    "lcov-report": "nyc report --reporter=lcov",
    "mocha": "cross-env NODE_ENV=test mocha -r ts-node/register -R spec ./test/*-test.ts --full-trace --exit",
    "generate": "protoc --ts_proto_opt=outputClientImpl=false,env=node,useOptionals,outputJsonMethods=true,snakeToCamel=false,esModuleInterop=true -I=./node_modules/@restorecommerce/protos --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated ./node_modules/@restorecommerce/protos/io/restorecommerce/fulfillment*.proto ./node_modules/@restorecommerce/protos/io/restorecommerce/filter.proto"
  },
  "engines": {
    "node": ">= 9.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
