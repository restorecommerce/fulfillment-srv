{
  "name": "fulfillments-srv",
  "version": "0.0.15",
  "main": "./lib/start.js",
  "description": "fulfillment service for creating and managing fulfillment",
  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/fulfillment-srv"
  },
  "dependencies": {
    "@restorecommerce/chassis-srv": "^0.1.6",
    "@restorecommerce/cluster-service": "^0.1.4",
    "@restorecommerce/kafka-client": "^0.1.12",
    "@restorecommerce/logger": "^0.3.5",
    "@restorecommerce/protos": "0.0.12",
    "@restorecommerce/resource-base-interface": "^0.0.5",
    "@restorecommerce/service-config": "^0.3.1",
    "@types/request": "^2.48.3",
    "@types/soap": "^0.21.0",
    "co": "^4.6.0",
    "cross-env": "^6.0.3",
    "html2json": "^1.0.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-rest-client": "^3.1.0",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "soap": "^0.30.0",
    "tslint": "^5.20.1",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.11",
    "@types/redis": "^2.8.14",
    "eslint": "^6.7.0",
    "eslint-plugin-prefer-arrow-functions": "^3.0.1",
    "mocha": "^6.2.1",
    "nodemon": "^2.0.1",
    "should": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "scripts": {
    "start": "npm run createtopics && node lib/start.js",
    "dev": "cross-env NODE_ENV=development npm run createtopics && nodemon --watch './src/**/*.ts' --exec 'ts-node' src/start.ts",
    "lint": "eslint src --ext .ts --fix",
    "createtopics": "node setupTopics io.restorecommerce.fulfillment.resource",
    "build:tsc": "tsc -d",
    "build:clean": "rimraf lib",
    "build": "npm-run-all lint build:clean build:tsc",
    "pretest": "npm run build && npm run tsctests && npm run createtopics",
    "test": "npm run lint && nyc npm run mocha",
    "tsctests": "tsc -d -p tsconfig.test.json",
    "mocha": "cross-env NODE_ENV=test mocha -R spec ./test/*.js --full-trace --exit"
  },
  "engines": {
    "node": ">= 9.2.0"
  }
}
