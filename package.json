{
  "name": "@restorecommerce/fulfillments-srv",
  "version": "0.5.3",
  "main": "./lib/start.js",
  "description": "fulfillment service for creating and managing fulfillments",
  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/fulfillment-srv.git"
  },
  "type": "module",
  "dependencies": {
    "@restorecommerce/acs-client": "^3.1.6",
    "@restorecommerce/cart": "^1.0.9",
    "@restorecommerce/chassis-srv": "^1.7.8",
    "@restorecommerce/grpc-client": "^2.2.13",
    "@restorecommerce/kafka-client": "^1.3.6",
    "@restorecommerce/logger": "^1.3.8",
    "@restorecommerce/rc-grpc-clients": "^5.1.62",
    "@restorecommerce/resource-base-interface": "^1.9.8",
    "@restorecommerce/scs-jobs": "^0.2.6",
    "@restorecommerce/service-config": "^1.1.7",
    "bignumber.js": "^9.3.1",
    "csv-parse": "^6.1.0",
    "dayjs": "^1.11.18",
    "lodash-es": "^4.17.21",
    "openapi-fetch": "^0.15.0",
    "redis": "^4.7.0",
    "soap": "<=1.1.6",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@alenon/grpc-mock-server": "^3.2.3",
    "@types/soap": "<=0.18.0",
    "@types/xml-js": "^1.0.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@restorecommerce/dev": "^0.2.0",
    "@semantic-release-plus/docker": "^3.1.3",
    "@types/ioredis": "^5.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/mocha": "^10.0.10",
    "@types/nconf": "^0.10.7",
    "@types/node": "^24.9.1",
    "async-mutex": "^0.5.0",
    "c8": "^10.1.3",
    "commitizen": "^4.3.1",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "esbuild": "^0.25.11",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "mocha": "^11.7.4",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^7.10.1",
    "rimraf": "^6.0.1",
    "should": "^13.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "scripts": {
    "start": "node lib/start.cjs",
    "dev": "cross-env NODE_ENV=development nodemon --watch './src/**/*.ts' --exec node --loader ts-node/esm ./src/start.ts",
    "lint": "eslint src --ignore-pattern **/*.d.ts",
    "build:es": "node esbuild.mjs",
    "build:jobs": "esbuild ./src/jobs/*.ts --bundle --platform=node --outdir=lib/jobs/ --minify --tree-shaking=true --sourcemap=inline --out-extension:.js=.cjs",
    "build:clean": "rimraf lib",
    "build": "npm-run-all build:clean build:es build:jobs",
    "test": "npm-run-all lint mocha",
    "mocha": "c8 --reporter=text cross-env NODE_ENV=test mocha --full-trace --exit --timeout 30000 -r dotenv/config",
    "lcov-report": "c8 report --reporter=lcov",
    "prepare": "npm-run-all install:husky generate:dhl:rest",
    "install:husky": "node -e \"try { require('husky').install() } catch (e) {}\"",
    "generate:dhl:rest": "openapi-typescript ./api/dhl/rest/parcel-de-shipping-v2.yaml -o src/adapters/dhl/rest/schema.d.ts"
  },
  "engines": {
    "node": ">= 20.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release-plus/docker",
        {
          "name": "docker.io/restorecommerce/fulfillment-srv",
          "skipLogin": true
        }
      ],
      [
        "@semantic-release-plus/docker",
        {
          "name": "ghcr.io/restorecommerce/fulfillment-srv",
          "skipLogin": true
        }
      ]
    ]
  }
}
