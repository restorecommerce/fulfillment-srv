{
  "name": "fulfillments-srv",
  "version": "0.0.1",
  "main": "./lib/start.js",
  "description": "fulfillment service for creating and managing fulfillment",
  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/fulfillment-srv"
  },
  "dependencies": {
    "@restorecommerce/cart": "^0.3.2",
    "@restorecommerce/chassis-srv": "^0.3.10",
    "@restorecommerce/cluster-service": "^0.1.10",
    "@restorecommerce/grpc-client": "^0.2.14",
    "@restorecommerce/kafka-client": "^0.2.29",
    "@restorecommerce/logger": "^0.11.3",
    "@restorecommerce/protos": "^0.4.11",
    "@restorecommerce/resource-base-interface": "^0.2.2",
    "@restorecommerce/service-config": "^0.4.23",
    "@types/node-fetch": "^1.6.9",
    "@types/soap": "^0.21.0",
    "@types/xml-js": "^1.0.0",
    "co": "^4.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "html2json": "^1.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.5",
    "protoc": "^1.0.4",
    "redis": "^3.1.2",
    "request": "^2.88.2",
    "soap": "^0.37.0",
    "tslint": "^6.1.3",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.3",
    "@types/node": "^14.17.19",
    "@types/redis": "^2.8.32",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prefer-arrow-functions": "^3.0.1",
    "grpc-mock": "^0.7.0",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "should": "^13.2.3",
    "ts-node": "^9.1.1",
    "ts-proto": "^1.65.0",
    "ts-proto-descriptors": "^1.2.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "start": "npm run createtopics && node lib/start.js",
    "dev": "cross-env NODE_ENV=development npm run createtopics && nodemon --watch './src/**/*.ts' --exec 'ts-node' src/start.ts",
    "lint": "eslint src --ext .ts --fix",
    "createtopics": "node setupTopics io.restorecommerce.fulfillment.resource io.restorecommerce.command io.restorecommerce.jobs io.restorecommerce.fulfillment_method.resource io.restorecommerce.fulfillment_courier.resource",
    "build:tsc": "tsc -d",
    "build:clean": "rimraf lib",
    "build": "npm-run-all lint build:clean build:tsc",
    "pretest": "npm run build && npm run tsctests && npm run createtopics",
    "test": "npm run lint && nyc npm run mocha",
    "tsctests": "tsc -d -p tsconfig.test.json",
    "lcov-report": "nyc report --reporter=lcov",
    "mocha": "cross-env NODE_ENV=test mocha -r ts-node/register -R spec ./test/*-test.ts --full-trace --exit",
    "generate": "protoc --ts_proto_opt=outputClientImpl=false,env=node,useOptionals=true,outputJsonMethods=true,outputSchema=true,snakeToCamel=false --plugin=./node_modules/.bin/protoc-gen-ts_proto --proto_path=./node_modules/@restorecommerce/protos --ts_proto_out=./src/generated ./node_modules/@restorecommerce/protos/io/restorecommerce/fulfillment*.proto ./node_modules/@restorecommerce/protos/io/restorecommerce/filter.proto"
  },
  "engines": {
    "node": ">= 9.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
