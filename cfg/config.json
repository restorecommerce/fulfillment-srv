      "defaults": {
        "ordering": {
          "wsdl": "./wsdl/dhl/geschaeftskundenversand-api-3.4.0.wsdl",
          "version": [3, 4, 0],
          "endpoint": "https://cig.dhl.de/services/sandbox/soap",
          "username": null,
          "password": null,
          "wsdl_header": {
            "Authentification": {
              "user": "2222222222_01",
              "signature": "pass"
            }
          }
        },
        "tracking": {
          "version": [3, 4, 0],
          "appname": "zt12345",
          "endpoint": "https://cig.dhl.de/services/sandbox/soap",
          "username": null,
          "password": null
        }
      }
    }
  }
}
