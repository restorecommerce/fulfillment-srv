{
  "logger": {
    "console": {
      "handleExceptions": false,
      "level": "crit",
      "colorize": true,
      "prettyPrint": true
    }
  },
  "database": {
    "main": {
      "provider": "arango",
      "host": "localhost",
      "port": 8529,
      "database": "restorecommerce-fulfillment-srv",
      "autoCreate": true,
      "collections": [
        "fulfillments",
        "fulfillment_couriers",
        "fulfillment_products"
      ],
      "entities": [
        "fulfillment",
        "fulfillment_courier",
        "fulfillment_product"
      ]
    }
  },
  "redis": {
    "host": "localhost",
    "port": 6379,
    "offsetStoreInterval": 1000
  },
  "client": {
    "fulfillment": {
      "address": "localhost:50067"
    },
    "fulfillment_courier": {
      "address": "localhost:50067"
    },
    "fulfillment_product": {
      "address": "localhost:50067"
    },
    "reflection": {
      "address": "localhost:50051"
    },
    "commandinterface": {
      "address": "localhost:50051"
    }
  },
  "events": {
    "kafka": {
      "provider": "kafka",
      "groupId": "restore-fulfillment-srv-test",
      "latestOffset": false,
      "kafka": {
        "clientId": "restore-fulfillment-srv-test",
        "brokers": [
          "localhost:29092"
        ]
      },
      "fulfillmentCreated": {
        "messageObject": "io.restorecommerce.fulfillment.Fulfillment"
      },
      "fulfillmentModified": {
        "messageObject": "io.restorecommerce.fulfillment.Fulfillment"
      },
      "fulfillmentCancelled": {
        "messageObject": "io.restorecommerce.fulfillment.Fulfillment"
      },
      "fulfillmentDeleted": {
        "messageObject": "io.restorecommerce.fulfillment.Deleted"
      },
      "fulfillmentFulfilled": {
        "messageObject": "io.restorecommerce.fulfillment.Fulfillment"
      },
      "fulfillmentLabelSubmitted": {
        "messageObject": "io.restorecommerce.fulfillment.Label"
      },
      "fulfillmentLabelInvalid": {
        "messageObject": "io.restorecommerce.fulfillment.Label"
      },
      "fulfillmentLabelShipping": {
        "messageObject": "io.restorecommerce.fulfillment.Label"
      },
      "fulfillmentLabelDone": {
        "messageObject": "io.restorecommerce.fulfillment.Label"
      },
      "fulfillmentLabelFailed": {
        "messageObject": "io.restorecommerce.fulfillment.Label"
      },
      "fulfillmentCourierCreated": {
        "messageObject": "io.restorecommerce.fulfillment_courier.FulfillmentCourier"
      },
      "fulfillmentCourierModified": {
        "messageObject": "io.restorecommerce.fulfillment_courier.FulfillmentCourier"
      },
      "fulfillmentCourierDeleted": {
        "messageObject": "io.restorecommerce.fulfillment_courier.Deleted"
      },
      "jobScheduled": {
        "protos": [
          "io/restorecommerce/job.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.job.ScheduledJob"
      },
      "jobDone": {
        "protos": [
          "io/restorecommerce/job.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.job.JobDone"
      },
      "jobFailed": {
        "protos": [
          "io/restorecommerce/job.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.job.JobFailed"
      },
      "healthCheckCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "healthCheckResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "versionCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "versionResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "restoreCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "restoreResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "resetCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "resetResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "topics": {
        "command": {
          "topic": "io.restorecommerce.command",
          "events": [
            "healthCheckCommand",
            "versionCommand",
            "restoreCommand",
            "resetCommand"
          ]
        },
        "jobs": {
          "topic": "io.restorecommerce.jobs",
          "events": [
            "queuedJob"
          ]
        },
        "fulfillment_courier.resource": {
          "topic": "io.restorecommerce.fulfillment_courier.resource"
        },
        "fulfillment_product.resource": {
          "topic": "io.restorecommerce.fulfillment_product.resource"
        },
        "fulfillment.resource": {
          "topic": "io.restorecommerce.fulfillment.resource",
          "events": [
            "createFulfillments",
            "trackFulfillments",
            "cancelFulfillments"
          ]
        }
      }
    },
    "enableEvents": true
  },
  "fulfillmentTrackingJob": "fulfillmentTrackingJob",
  "urns": {
    "ownerEntity": "urn:restorecommerce:acs:names:ownerIndicatoryEntity",
    "ownerInstance": "urn:restorecommerce:acs:names:ownerInstance",
    "organization": "urn:restorecommerce:acs:model:organization.Organization",
    "user": "urn:restorecommerce:acs:model:user.User",
    "productName": "urn:restorecommerce:fufs:names:product:attr:productName",
    "accountNumber": "urn:restorecommerce:fufs:names:product:attr:accountNumber"
  },
  "server": {
    "services": {
      "io-restorecommerce-fulfillment-srv": {
        "create": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        },
        "track": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        },
        "cancel": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        },
        "delete": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        }
      },
      "io-restorecommerce-fulfillment-courier-srv": {
        "create": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        },
        "update": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        },
        "upsert": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        },
        "delete": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        }
      },
      "io-restorecommerce-fulfillment-product-srv": {
        "find": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        },
        "create": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        },
        "update": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        },
        "upsert": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        },
        "delete": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        }
      },
      "io-restorecommerce-fulfillment-reflection": {
        "serverReflectionInfo": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        }
      },
      "io-restorecommerce-fulfillment-cis": {
        "command": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        }
      },
      "grpc-health-v1": {
        "check": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        },
        "watch": {
          "transport": [
            "grpcFulfillmentSrv"
          ]
        }
      }
    },
    "transports": [
      {
        "name": "grpcFulfillmentSrv",
        "provider": "grpc",
        "services": {
          "io-restorecommerce-fulfillment-srv": "io.restorecommerce.fulfillment.Service",
          "io-restorecommerce-fulfillment-courier-srv": "io.restorecommerce.fulfillment_courier.Service",
          "io-restorecommerce-fulfillment-product-srv": "io.restorecommerce.fulfillment_product.Service",
          "io-restorecommerce-fulfillment-reflection": "grpc.reflection.v1alpha.ServerReflection",
          "io-restorecommerce-fulfillment-cis": "io.restorecommerce.commandinterface.Service",
          "grpc-health-v1": "grpc.health.v1.Health"
        },
        "protos": [
          "io/restorecommerce/fulfillment.proto",
          "io/restorecommerce/fulfillment_courier.proto",
          "io/restorecommerce/fulfillment_product.proto",
          "grpc/reflection/v1alpha/reflection.proto",
          "io/restorecommerce/commandinterface.proto",
          "grpc/health/v1/health.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "addr": "localhost:50067"
      }
    ]
  },
  "serviceNames": {
    "fulfillment": "io-restorecommerce-fulfillment-srv",
    "fulfillment_courier": "io-restorecommerce-fulfillment-courier-srv",
    "fulfillment_product": "io-restorecommerce-fulfillment-product-srv",
    "cis": "io-restorecommerce-fulfillment-cis",
    "reflection": "io-restorecommerce-fulfillment-reflection",
    "health": "grpc-health-v1"
  },
  "fieldHandlers": {
    "fulfillment_courier": {
      "bufferField": "configuration"
    },
    "fulfillment": {
      "bufferField": "options"
    }
  },
  "stubs": {
    "DHL": {
      "ordering": {
        "wsdl": "./wsdl/dhl/geschaeftskundenversand-api-3.4.0.wsdl",
        "version": [3, 4, 0],
